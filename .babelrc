{
  "presets": [ "next/babel" ],
  "plugins": [
    [ 
      "styled-components", 
      { 
        "ssr": true, 
        "displayName": true, 
        "preprocess": false 
      } 
    ],
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./static/css",
          "components": "./components"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "ignore": []
}
