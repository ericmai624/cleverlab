{
  "presets": [
    "env",
    "next/babel"
  ],
  "plugins": [
    [ 
      "styled-components", 
      { 
        "ssr": true, 
        "displayName": true,
        "preprocess": false 
      } 
    ],
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "static": "./static",
          "components": "./components",
          "lib": "./lib",
          "api": "./api",
          "database": "./database"
        },
        "cwd": "babelrc"
    }]
  ],
  "ignore": []
}
